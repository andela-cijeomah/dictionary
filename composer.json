    "keywords": [
        "andela",
        "dictionary"
    ],
    "homepage": "https://github.com/andela-cijeomah/dictionary",
    "license": "MIT",
    "authors": [
        {
            "name": "Chidozie Ijeomah",
            "email": "chidozie.ijeomah@andela.com",
